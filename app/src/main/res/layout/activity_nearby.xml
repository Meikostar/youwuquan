<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    tools:context="com.ningchuang.youwuquan.ui.home.activity.NearbyActivity">
   <com.zqd.common.base.NavigationBar
       android:id="@+id/navigationBar"
       app:navigationTitle="附近的人"
       android:layout_width="match_parent"
       app:navigationRight_img="@mipmap/home_filtrate"
       app:navigationRight_type="img"
       android:layout_height="wrap_content"/>
    <!--<include layout="@layout/top_home_item" />-->

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rlv_nearby_people"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v7.widget.RecyclerView>
</LinearLayout>
